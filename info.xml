<?xml version="1.0"?>
<extension key="de.systopia.bpk" type="module">
  <file>bpk</file>
  <name>Austrian BMF</name>
  <description>Provides services around bPKs and SA submissions</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.bpk</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.bpk</url>
    <url desc="Support">https://github.com/systopia/de.systopia.bpk/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-02-10</releaseDate>
  <version>1.0-alpha.1.gp.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Funded by GP CEE</comments>
  <civix>
    <namespace>CRM/Bpk</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Bpk_Upgrader</upgrader>
</extension>
